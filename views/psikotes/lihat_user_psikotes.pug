extends ../layout
block nav 
    .column
        img(src="/images/Logo-Rajawali.png", alt="Logo Rajawali") 
    .row
        h2 Form input izin karyawan
        button#logout Logout
    .block
        nav(aria-label="Breadcrumb" class="breadcrumb")
            ul
                li
                    a(href="/kehadiran/info") Home
                li
                    span(aria-current="page") Psikotes
block info
    .block 
        .container2
            .header 
                h2 table_ist
            .body
                table#table_ist
                    tr 
                        th Nama
                        th Jenis Kelamin 
                        th Tanggal Lahir 
                        th Pendidikan Terakhir 
                        th Result 
        .container2
            .header 
                h2 table_disc
            .body
                table#table_disc
                    tr 
                        th Nama
                        th Jenis Kelamin 
                        th Umur 
                        th Result
block append script 
    script(src='/javascripts/getIST.js')
    script(src='/javascripts/getDISC.js')