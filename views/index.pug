extends layout

block content
  .header
    img(src="images/Logo-Rajawali.png", alt="Rajawali Logo")
  .block
    .container1
      .header 
        h2 Login
      .body
        form#form
          .form-group
            label(for='email') Nama
            input#email(type='text' name='email')
          .form-group
            label(for='password') Password
            input#password(type='password' name='password')
          button(type='submit') Submit
          a(href='/psikotes') Psikotes
  script(src='/javascripts/indexScript.js')